{
    "id": "gatsby-preview-sidebar",
    "name": "Gatsby Preview: Instant content updates",
    "srcdoc": "./build/index.html",
    "sidebar": true,
    "parameters": {
      "instance": [
        {
          "id": "contentTypeSlug",
          "type": "Symbol",
          "name": "URL slug for content type"
        },
        {
          "id": "isAutoUpdate",
          "type": "Boolean",
          "name": "Automatically update Gatsby Preview?",
          "default": true
        }
      ],
      "installation": [
        {
          "id": "previewUrl",
          "type": "Symbol",
          "name": "The url of your Gatsby Preview instance"
        },
        {
          "id": "webhookUrl",
          "type": "Symbol",
          "name": "The url of the webhook to rebuild your Gatsby Preview instance"
        }
      ]
    }
  }